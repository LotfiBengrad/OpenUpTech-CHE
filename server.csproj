  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="AWSSDK.Core" Version="3.3.107.21" />
    <PackageReference Include="AWSSDK.S3" Version="3.3.111.22" />
    <PackageReference Include="BCrypt.Net-Next" Version="3.3.3" />
    <PackageReference Include="docfx.console" Version="2.56.5" />
    <PackageReference Include="Microsoft.AspNet.WebApi.Cors" Version="5.2.7" />
    <PackageReference Include="MongoDB.Driver" Version="2.11.0-beta1" />
  </ItemGroup>
  <ItemGroup>
    <Content Remove="_doc_site\manifest.json" />
    <Content Remove="_doc_site\search-stopwords.json" />
  </ItemGroup>
</Project>
