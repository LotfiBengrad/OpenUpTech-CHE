  </ItemGroup>
  <ItemGroup>
    <Compile Remove="OpenUp DLLs\**" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Remove="OpenUp DLLs\**" />
  </ItemGroup>
  <ItemGroup>
	<Content Include="_doc_site\**" CopyToPublishDirectory="PreserveNewest" />
