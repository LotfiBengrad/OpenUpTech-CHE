<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <BuildDocFx>false</BuildDocFx>
    <UserSecretsId>c5896512-4dd1-4d17-9742-998e67e25fae</UserSecretsId>
  </PropertyGroup>
  <ItemGroup>
    <Reference Include="NetworkConnection, Version=3.14.15.92, Culture=neutral, PublicKeyToken=null">
      <HintPath>OpenUp DLLs/NetworkConnection.dll</HintPath>
    </Reference>
    <Reference Include="OpenUpDataStructures">
      <HintPath>OpenUp DLLs\OpenUpDataStructures.dll</HintPath>
    </Reference>
	<Reference Include="UnityEngine, Version=0.0.0.0, Culture=neutral, PublicKeyToken=null">
      <HintPath>OpenUp DLLs\UnityEngine.dll</HintPath>
    </Reference>
  </ItemGroup>
  <ItemGroup>
    <None Remove="OpenUp DLLs\UnityEngine.dll" />
    <None Remove="OpenUp DLLs\OpenUpDataStructures.dll" />
    <None Remove="OpenUp DLLs\NetworkConnection.dll" />
